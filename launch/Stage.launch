<launch>
	<arg name="map_name" default="DIAG_B1" />
	<param name="/use_sim_time" value="true"/>
	
	<node pkg="stage_environments" type="stageros" name="stageros" args="-u $(find WaypointNavigation)/maps/$(arg map_name).world" respawn="false" output="screen">
		<param name="base_watchdog_timeout" value="0.2"/>
		<param name="base_frame" value="base_link"/>
		<param name="laser_topic" value="scan"/>
		<param name="laser_frame" value="base_laser_link"/>
	</node>
</launch>
